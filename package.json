{
  "author": "jamuhl",
  "name": "i18next-conv",
  "description": "converts .mo and .po files to i18next's json format and vice versa.",
  "keywords": [
    "i18next",
    "gettext"
  ],
  "version": "12.1.1",
  "type": "module",
  "engines": {
    "node": ">= 12.2"
  },
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "i18next-conv": "bin/index.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/i18next/i18next-gettext-converter.git"
  },
  "license": "SEE LICENSE IN LICENSE.MD",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --cache .",
    "test": "mocha",
    "cover": "c8 npm test",
    "prepare": "npm run build"
  },
  "dependencies": {
    "c8": "^7.11.3",
    "colorette": "^2.0.16",
    "commander": "^9.2.0",
    "gettext-converter": "^1.2.2",
    "gettext-parser": "^5.1.2",
    "node-gettext": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.0.4",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^10.0.0",
    "rollup": "^2.75.0"
  }
}
